enum ModelMixingMode: byte {
  WeightedSum,
  AddDifference,
  Freeform
}

table ModelMixingItem {
  name: string;
  weight: float;
}

table ModelMixingMetadata {
  name: string (primary);
  trigger_word: string;
  v_prediction: bool;
  upcast_attention: bool;
  mode: ModelMixingMode;
  items: [ModelMixingItem];
  note: string;
  encoder: string;
  decoder: string;
}

root_type ModelMixingMetadata;
